<?xml version='1.0' encoding='utf-8'?>
<widget id="iam.mitkko.csdpeek" version="1.1.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="File">
        <param name="android-package" value="org.apache.cordova.file.FileUtils" />
        <param name="onload" value="true" />
    </feature>
    <allow-navigation href="cdvfile:*" />
    <name>csdpeek</name>
    <description>
        A ClubSoccerDirector game read-only save file viewer.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="market:*" />
    <icon density="ldpi" src="res/icon/android/peek.png" />
    <icon density="mdpi" src="res/icon/android/peek.png" />
    <icon density="hdpi" src="res/icon/android/peek.png" />
    <icon density="xhdpi" src="res/icon/android/peek.png" />
    <icon density="xxhdpi" src="res/icon/android/peek.png" />
    <icon density="xxxhdpi" src="res/icon/android/peek.png" />
    <preference name="loglevel" value="DEBUG" />
</widget>
